{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "SecretWall DEV",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_dev.dart",
            "args": [
                "--flavor",
                "dev"
            ]
        },
        {
            "name": "SecretWall QA",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_qa.dart",
            "args": [
                "--flavor",
                "qa"
            ]
        },
        {
            "name": "SecretWall PROD",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_prod.dart",
            "args": [
                "--flavor",
                "prod"
            ]
        },
        // Sometimes it throws error in android because flutter need all flavor release build to run correctly. 
        {
            "name": "SecretWall PROD release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main-prod.dart",
            "args": [
                "--flavor",
                "prod"
            ]
        }
    ]
}